{
  "apps": [
    {
    "name": "print",
    "cwd": "/root/print/printer",
    "script": "consumer.js",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "error_file": "/root/print/printer/log/node-print.stderr.log",
    "out_file": "/root/print/printer/log/log/node-print.stdout.log",
    "pid_file": "/root/print/printer/log/pids/node-print-api.pid",
    "instances": 1,
    "min_uptime": "2s",
    "max_restarts": 0,
    "max_memory_restart": "280M",
    "cron_restart": "1 0 * * *",
    "watch": false,
    "merge_logs": true,
    "exec_interpreter": "node",
    "exec_mode": "fork",
    "autorestart": true,
    "vizion": false
    }
  ]
 }